    "@tauri-store/vue": "^0.3.0",
    "@tb-dev/utils": "^7.0.5",
    "@tb-dev/vue": "^2.0.10",
    "@tb-dev/vue-components": "^2.2.4",
    "@vueuse/core": "^13.5.0",
    "lucide-vue-next": "^0.525.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tb-dev/eslint-config": "^7.1.10",
