{
  "name": "tauri-vue-template",
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@10.12.4",
  "private": true,
  "scripts": {
    "build": "cargo tauri build",
    "clippy": "cargo +nightly clippy --workspace",
